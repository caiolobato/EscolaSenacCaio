<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pa.senac.br.escolasenaccaio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AlunoActivity"
            android:label="@string/activityTitulo_Aluno"
            android:parentActivityName="pa.senac.br.escolasenaccaio.MainActivity">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="pa.senac.br.escolasenaccaio.MainActivity" />
        </activity>
        <activity android:name=".AlunoCadastraActivity" />

        <activity
            android:name=".CursoActivity"
            android:label="@string/activityTitulo_Curso"
            android:parentActivityName="pa.senac.br.escolasenaccaio.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pa.senac.br.escolasenaccaio.MainActivity" />
        </activity>
        <activity android:name=".CursoCadastraActivity" />

    </application>

</manifest>